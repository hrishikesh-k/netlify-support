{
  "compilerOptions": {
    "allowJs": false,
    "allowUnusedLabels": false,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "paths": {
      "~/client/*": [
        "./src/*"
      ],
      "~/server/*": [
        "./netlify/functions/*"
      ],
      "~/types/*": [
        "./@types/*"
      ]
    },
    "skipLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "verbatimModuleSyntax": true
  },
  "include": [
    "./@types/**/*.ts",
    "./netlify/functions/**/*.ts",
    "./src/**/*.ts",
    "./src/**/*.vue",
    "./histoire.config.ts",
    "./histoire.setup.ts",
    "./uno.config.ts",
    "./vite.config.ts"
  ]
}